{
  "version": "1.2.0-*",
  "description": "A generic hosting infrastructure and startup logic for applications.",
  "packOptions": {
    "repository": {
      "type": "git",
      "url": "git://github.com/aspnet/hosting"
    },
    "tags": [
      "aspnetcore",
      "hosting"
    ]
  },
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "../../tools/Key.snk",
    "nowarn": [
      "CS1591"
    ],
    "xmlDoc": true
  },
  "dependencies": {
    "Microsoft.Extensions.Hosting.Abstractions": "1.0.0-*",
    "Microsoft.Extensions.Configuration": "1.2.0-*",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.2.0-*",
    "Microsoft.Extensions.DependencyInjection": "1.2.0-*",
    "Microsoft.Extensions.FileProviders.Physical": "1.2.0-*",
    "Microsoft.Extensions.Logging": "1.2.0-*",
    "Microsoft.Extensions.Options": "1.2.0-*",
    "Microsoft.Extensions.PlatformAbstractions": "1.2.0-*",
    "NETStandard.Library": "1.6.1-*",
    "System.Diagnostics.DiagnosticSource": "4.3.0-*"
  },
  "frameworks": {
    "net451": {
      "frameworkAssemblies": {
        "System.Runtime": {
          "type": "build"
        },
        "System.ComponentModel": {
          "type": "build"
        },
        "System.IO": {
          "type": "build"
        }
      }
    },
    "netstandard1.3": {
    },
    "netstandard1.5": {
      "dependencies": {
        "System.Runtime.Loader": "4.3.0-*"
      }
    }
  }
}